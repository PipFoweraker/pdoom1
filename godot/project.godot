; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="P(Doom)"
config/description="AI Safety Lab Management Simulation - Strategic resource allocation and risk management"
run/main_scene="res://scenes/welcome.tscn"
config/features=PackedStringArray("4.5")

[autoload]

ErrorHandler="*res://autoload/error_handler.gd"
GameConfig="*res://autoload/game_config.gd"
SceneTransition="*res://autoload/scene_transition.gd"
ThemeManager="*res://autoload/theme_manager.gd"
NotificationManager="*res://autoload/notification_manager.gd"
KeybindManager="*res://autoload/keybind_manager.gd"
ScreenshotManager="*res://autoload/screenshot_manager.gd"
LogExporter="*res://autoload/log_exporter.gd"
IconLoader="*res://autoload/icon_loader.gd"
MusicManager="*res://autoload/music_manager.gd"
VerificationTracker="*res://autoload/verification_tracker.gd"
SteamManager="*res://autoload/steam_manager.gd"
GameManager="*res://scripts/game_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray()

[rendering]

textures/vram_compression/import_etc2_astc=true
environment/default_environment="res://default_env.tres"
